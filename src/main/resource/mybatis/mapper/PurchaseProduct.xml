        </if>
        <if test="updateTime != null">
           and UPDATE_TIME = #updateTime#
        </if>
        <if test="remark != null">
           and REMARK = #remark#
        </if>
        <if test="delFlag != null">
           and DEL_FLAG = #delFlag#
        </if>
        <if test="productName != null">
           and PRODUCT_NAME = #productName#
        </if>
        <if test="totalAmount != null">
           and TOTAL_AMOUNT = #totalAmount#
        </if>
        <if test="unit != null">
           and UNIT = #unit#
        </if>
        <if test="price != null">
